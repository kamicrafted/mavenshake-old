{
  "require": {
    "aelvan/imager": "v2.3.0",
    "chasegiunta/scss": "1.0.0",
    "craftcms/cms": "3.3.13",
    "craftcms/commerce": "2.2.6",
    "craftcms/commerce-stripe": "2.2.1",
    "craftcms/element-api": "2.6.0",
    "craftcms/redactor": "2.4.0",
    "fostercommerce/shipstationconnect": "1.2.4",
    "la-haute-societe/craft-google-tag-manager": "1.1.0",
    "nystudio107/craft-instantanalytics": "1.1.0",
    "olivestudio/craft-olivemenus": "1.0.9",
    "topshelfcraft/wordsmith": "3.1.1",
    "verbb/commerce-payflow": "2.0.0",
    "verbb/cp-nav": "2.0.9",
    "verbb/expanded-singles": "1.0.7",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
