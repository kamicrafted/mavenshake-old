{
  "require": {
    "aelvan/imager": "v2.1.10",
    "chasegiunta/scss": "1.0.0",
    "craftcms/cms": "^3.0.0",
    "craftcms/commerce": "2.1.11",
    "craftcms/commerce-stripe": "2.1.0",
    "fostercommerce/shipstationconnect": "1.1.0",
    "la-haute-societe/craft-google-tag-manager": "1.1.0",
    "nystudio107/craft-instantanalytics": "1.1.0",
    "olivestudio/craft-olivemenus": "1.0.9",
    "topshelfcraft/wordsmith": "3.0.5",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
